# Database Configuration
# Copy this file to .env and modify values as needed

# MySQL Database Host (use 'mysql' for Docker Compose, 'localhost' for local development)
DB_HOST=mysql

# MySQL Database Port
DB_PORT=3306

# Database Name
DB_NAME=helpdesk

# Database User
DB_USER=root

# Database Password (CHANGE THIS IN PRODUCTION!)
DB_PASSWORD=A_1234567

# MySQL Root Password (for MySQL container)
MYSQL_ROOT_PASSWORD=A_1234567

# MySQL Database (for MySQL container)
MYSQL_DATABASE=helpdesk
